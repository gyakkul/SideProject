						<Directory Id="CoreStyleFolder" />
					</Directory>
					<Directory Id="SHADERSFOLDER" Name="Shaders">
						<Directory Id="STANDALONERENDERERFOLDER" Name="StandaloneRenderer">
							<Directory Id="D3DShadersFolder" />
						</Directory>
					</Directory>
				</Directory>
			</Directory>
