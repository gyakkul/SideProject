				<Directory Id="INSTALL2021ADDIN" Name="2021">
					<Directory Id="INSTALL2021FOLDER" Name="DatasmithRevit2021">
						<Directory Id="LOCALIZATION_DE_2021FOLDER" Name="de" />
						<Directory Id="LOCALIZATION_ES_2021FOLDER" Name="es" />
