								<Directory Id="INSTALLYYY2017DIR_UI_ln_Resources_Light_Icons_Datasmith" Name="Datasmith">
								</Directory>
							</Directory>
						</Directory>
					</Directory>
				</Directory>
				<Directory Id="INSTALLXXX2017DIR_de_DE" Name="de-DE">
					<Directory Id="INSTALLXXX2017DIR_de_DE_UI" Name="UI">
						<Directory Id="INSTALLXXX2017DIR_de_DE_UI_Ribbon" Name="Ribbon">
							<Directory Id="INSTALLXXX2017DIR_de_DE_UI_Ribbon_Extensions" Name="Extensions">
