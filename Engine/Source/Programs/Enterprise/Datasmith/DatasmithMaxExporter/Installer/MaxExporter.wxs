		<SetProperty Id="MAX2022CHECKED" After="AppSearch" Value="1">MAX2022VALID</SetProperty>
		<SetProperty Id="MAX2021CHECKED" After="AppSearch" Value="1">MAX2021VALID</SetProperty>
