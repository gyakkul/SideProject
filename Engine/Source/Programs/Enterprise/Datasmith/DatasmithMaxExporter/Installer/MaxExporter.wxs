		<Property Id="MAX2023VALID">
			<DirectorySearch Id="CheckMax2023Dir" Path="[MAX2023DIR]" Depth="0">
				<FileSearch Id="CheckMax2023File" Name="3dsmax.exe" />
			</DirectorySearch>
		</Property>
